<div id="change-session-<%-entitlementUUID%>" class="message is-shown">
    <div class="action-header">
        <% if (currentSessionId) { %>
            <%- gettext('To change your session or leave your current session, please select from the following:')%>
        <% } else { %>
            <%- gettext('In order to view the course you must select a session:')%>
        <% } %>
    </div>
    <div class="action-controls">
        <select class="session-select">
            <% for (var i=0; i< availableSessions.length; i++) { %>
                <option data-session_id="<%-availableSessions[i].session_id%>">
                    <%-availableSessions[i].session_dates%>
                    <% if (availableSessions[i].session_id == currentSessionId) { %>
                        <%- gettext('  - Currently Enrolled')%>
                    <% } else if (availableSessions[i].enrollment_end){ %>
                        <%- gettext(' (Enroll by ') + availableSessions[i].enrollment_end + ')'%>
                    <% } %>
                </option>
            <% } %>
            <% if (currentSessionId){%> <option><%- gettext('Leave current session and decide later.')%></option><%}%>
            <% if (!currentSessionId && availableSessions.length == 1){%> <option disabled><%- gettext('More sessions coming soon')%></option><%}%>
        </select>
        <button class="enroll-btn-initial"> <%- gettext('Enroll In Session') %></button>
    </div>
</div>
